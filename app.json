{
  "name": "Census Events",
  "description": "Find local resources and events in order to take the 2020 Census.",
  "repository": "https://github.com/openoakland/dos-census-events",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "11.5"
      }
    }
  ],
  "env": {
    "SECRET_KEY": {
      "description": "A secret key to provide cryptographic signing for Django.",
      "generator": "secret"
    },
    "GOOGLE_CALENDAR_ID": {
      "description": "Google Calendar Id where events will be published.",
      "value": ""
    },
    "GOOGLE_SERVICE_ACCOUNT_INFO": {
      "description": "Google Service Account credentials. See https://github.com/openoakland/dos-census-events#setup-google-calendar",
      "value": ""
    },
    "ALLOWED_HOSTS": ".herokuapp.com",
    "DEBUG": "0"
  },
  "keywords": [
    "calendar",
    "census",
    "django",
    "events",
    "python"
  ]
}
